@model ConferenceProject.Models.ApplicationUser

@{
    ViewData["Title"] = "ApplicationUserDelete";
}

<h2>ApplicationUserDelete</h2>

<div class="row">
    <div class="col-md-8">
        <section>
            <div id="panel" class="col-sm-6" style="width:auto">
                <div class="panel">
                    <div class="panel-body p-t-10">


                        <h3>Are you sure you want to delete this user?</h3>
                        <div>
                            
                            <hr />
                            <dl class="dl-horizontal">




                                <dt>
                                    Email
                                 </dt>
                                <dd>
                                    @Html.DisplayFor(model => model.Email)
                                </dd>


                                <dt>
                                   Address </dt>
                                <dd>
                                    @Html.DisplayFor(model => model._address)
                                </dd>

                                <dt>
                                   First Name</dt>
                                <dd>
                                    @Html.DisplayFor(model => model._fname)
                                </dd>
                                <dt>
                                    Last Name</dt>
                                <dd>
                                    @Html.DisplayFor(model => model._lname)
                                </dd>
                                <dt>
                                   Register Time
                                </dt>
                                <dd>
                                    @Html.DisplayFor(model => model._registerDate)
                                </dd>
                            </dl>

                            <form asp-action="ApplicationUserDelete">
                                <div class="form-actions no-color">
                                    <input type="submit" value="Delete" class="btn btn-default" /> |
                                    <a asp-action="UsersManager">Back to List</a>
                                </div>
                            </form>
                        </div>

                        </div>
                    </div>
                </div>
            </section>
        </div>
    </div>
